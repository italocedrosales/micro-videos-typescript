{
  "env": {
    "es2021": true,
    "node": true
  },
  "extends": [
    "airbnb-base"
  ],
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "plugins": [
    "@typescript-eslint"
  ],
  "rules": {
    "import/prefer-default-export": "off",
    "no-useless-constructor": "off",
    "no-unused-vars": "off",
    "no-empty-function": "off",
    "no-undef": "off",
    "import/no-unresolved": "off",
    "import/extensions": "off"
  }
}
