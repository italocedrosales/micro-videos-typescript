    "@typescript-eslint/eslint-plugin": "^5.28.0",
    "@typescript-eslint/parser": "^5.28.0",
    "eslint": "^8.17.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.26.0",
    "git-commit-msg-linter": "^4.1.3",
    "husky": "^4.3.8",
    "jest": "^28.1.1",
    "lint-staged": "^13.0.1",
    "ts-jest": "^28.0.5",
    "ts-node-dev": "^2.0.0",
